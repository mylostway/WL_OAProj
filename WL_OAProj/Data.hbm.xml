<?xml version="1.0" encoding="utf-8" ?>

<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" assembly="Data" namespace="Data.entity" default-lazy="true">

  <class name="DriverInfoEntity" table="t_driverInfo">
